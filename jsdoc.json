{
    "opts": {
        "destination": "./doc/"
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": "ssc\\..+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include" : ["./", "./README.md"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "default" : {
            "layoutFile": "./layout.tmpl"
        },
        "monospaceLinks": false
    }
}
